{
  "compilerOptions": {
    "lib": ["es2019"],
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "target": "es2019",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["nodes", "credentials", "AliExpressSDK.ts", "aliexpress.svg"],
  "exclude": [
      "node_modules", 
      "**/*.spec.ts"
  ]
}